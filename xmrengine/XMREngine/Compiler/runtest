#!/bin/bash -v
set -e
time make xmrengtest.exe
export MONO_PATH=../../../bin
export PATH=~/mono64.267/bin:$PATH

mono --debug xmrengtest.exe -checkrun -asm while1.xmrasm    while1.lsl
mono --debug xmrengtest.exe -checkrun -asm arraytest.xmrasm arraytest.lsl
mono --debug xmrengtest.exe -checkrun -asm objectest.xmrasm objectest.lsl
